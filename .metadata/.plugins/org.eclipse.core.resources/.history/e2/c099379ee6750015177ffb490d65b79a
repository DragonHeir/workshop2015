package Level_1;

public class methodpractice
{
	public static void main(String[] args)
	{
		methodpractice p = new methodpractice();
	}

	methodpractice()
	{
		add10(9);
		exclamation("Hello World");
	}

	int add10(int a)
	{
		return a + 10;
	}

	void exclamation(String b)
	{
		System.out.println(b + "!");
	}
}
