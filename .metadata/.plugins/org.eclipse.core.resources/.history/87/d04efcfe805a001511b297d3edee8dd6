	JPanel Panel;
	JTextField Decoded;
	JTextField Encoded;
	JButton Translate;

	public static void main(String[] args)
	{
		StringFun SF = new StringFun();
	}

	public StringFun()
	{
		Frame = new JFrame();
		Panel = new JPanel();
		Decoded = new JTextField(100);
		Encoded = new JTextField(100);
		Translate = new JButton();

		Frame.add(Panel);
		Panel.add(Decoded);
		Panel.add(Encoded);
		Panel.add(Translate);

		Frame.setVisible(true);
		Frame.setSize(0, 0);
		Frame.pack();

		// String word =
